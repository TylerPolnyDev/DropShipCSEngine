<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/cron_groups.xsd">
<group id="REDACTED_order_management">
    <schedule_generate_every>1</schedule_generate_every>
    <schedule_ahead_for>300</schedule_ahead_for>
    <schedule_lifetime>300</schedule_lifetime>
    <history_cleanup_every>300</history_cleanup_every>
    <history_success_lifetime>300</history_success_lifetime>
    <history_failure_lifetime>300</history_failure_lifetime>
    <use_separate_process>1</use_separate_process>
</group>
</config>
